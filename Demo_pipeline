pipeline{
    agent any
    
    stages{
        stage('compile'){
            steps{
                echo "printing java program"
                echo "Ankit running pipelibe"
            sh "javac Demo.java"
            }

        }
    
        stage('run'){
            steps{
            sh 'java Demo'
            }

        }
    }
    post{
        always{
            sh 'echo "printing always"'
        }
        success{
            sh 'echo "success code"'
        }
    }
    }